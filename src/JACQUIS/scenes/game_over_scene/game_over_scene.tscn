script/source = "extends Node2D


func _on_Btn_Recomecar_pressed():
	_save_score()
	Global.score = 0
	get_tree().change_scene(\"res://scenes/intro_scene/intro_scene.tscn\")


func _on_Btn_Desistir_pressed():
	_save_score()
	get_tree().change_scene(\"res://scenes/ranking_scene/ranking_scene.tscn\")

func _save_score():
	var ranking = File.new()
	ranking.open(\"user://ranking.save\", File.READ_WRITE)
	ranking.seek_end()
	ranking.store_line(\"%s,%s\" % [Global.nome, Global.score])
	ranking.close()
"

